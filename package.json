{
  "name": "book",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cz": "pnpm format && git add . && git cz",
    "czp": "pnpm cz && git push -u origin HEAD",
    "format": "pnpm lint --write",
    "lint": "prettier '**/*.{ts,tsx}' --list-different",
    "surge": "pnpm build && keke surge ",
    "surge:prod": "pnpm build && keke surge --prod"
  },
  "keke": {
    "group": "h5-okjike",
    "source": "dist",
    "codename": "jk-book",
    "prefix": "jk-book"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@ruguoapp/base-js-sdk": "^1.5.0",
    "@ruguoapp/jike-fe-components": "^1.2.0",
    "@ruguoapp/jike-js-sdk": "^1.6.1",
    "@ruguoapp/jk-analytics": "^1.4.19",
    "@ruguoapp/jk-theme": "^1.1.4",
    "@ruguoapp/jkfe-env": "^1.4.0",
    "@tanstack/react-query": "^4.0.10",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6",
