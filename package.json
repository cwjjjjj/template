    "@vitejs/plugin-react": "^2.0.0",
    "commitizen": "^4.2.5",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
